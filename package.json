{
  "name": "css-analyzer-diff",
  "version": "1.1.4",
  "description": "Calculate the difference between two sets of CSS stats ",
  "main": "diff.js",
  "engines": {
    "node": ">= 7"
  },
  "scripts": {
    "test": "ava && xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartveneman/css-analyzer-diff.git"
  },
  "keywords": [
    "css",
    "stats",
    "diff",
    "stylesheets",
    "analytics",
    "analysis",
    "statistics",
    "cli",
    "cli-app",
    "cssstats",
    "parker",
    "stylestats",
    "projectwallace",
    "wallace",
    "performance",
    "property"
  ],
  "author": "Bart Veneman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartveneman/css-analyzer-diff/issues"
  },
  "homepage": "https://github.com/bartveneman/css-analyzer-diff#readme",
  "dependencies": {
    "color-sorter": "^3.0.0",
    "css-unit-sort": "^1.1.1",
    "string-natural-compare": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "prettier": "^1.16.4",
    "xo": "^0.24.0"
  },
  "xo": {
    "semicolon": false,
    "prettier": true
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
